{
	"info": {
		"_postman_id": "d6f3e0c4-5255-4ce1-a896-79ca996fc99d",
		"name": "Vibent Back",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Test auth",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "a9c4a3e3-3082-499e-91ba-f520649a8005",
						"type": "text/javascript",
						"exec": [
							"var jsonData = JSON.parse(responseBody);",
							"postman.setEnvironmentVariable(\"token\", jsonData.token);"
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{ \r\n  \"username\" : \"vibentUser\",\r\n  \"password\" : \"vibentPass\"\r\n}"
				},
				"url": {
					"raw": "{{URL}}/auth",
					"host": [
						"{{URL}}"
					],
					"path": [
						"auth"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get User",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "{{TOKEN_HEADER}}",
						"value": "{{token}}"
					}
				],
				"body": {},
				"url": {
					"raw": "{{URL}}/user/5",
					"host": [
						"{{URL}}"
					],
					"path": [
						"user",
						"5"
					]
				}
			},
			"response": []
		}
	]
}